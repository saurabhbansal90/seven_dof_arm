<robot name="my_robot">
  
  <link name="base_link">
    <inertial>
      <mass value="2"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.100 0.300 0.050"/>
      </geometry>
      <material name="">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.100 0.300 0.050"/>
      </geometry>
    </collision>
  </link>
  
  <link name="child_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.200 0.050 0.025"/>
      </geometry>
      <material name="">
        <color rgba="1 0 0 1"/>
      </material>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.200 0.050 0.025"/>
      </geometry>
  </collision>
  </link>
  
  <joint name="base_to_child" type="revolute">
    <parent link="base_link"/>
    <child link="child_link"/>
    <origin xyz="0 0 0.0375" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-3.14159" upper="3.14159" velocity="3"/>
  </joint>
  
  <link name="child_link2">
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.100 0.025 0.005"/>
      </geometry>
      <material name="">
        <color rgba="1 0 0 1"/>
      </material>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.100 0.025 0.005"/>
      </geometry>
  </collision>
  </link>
  
  <joint name="child_to_child2" type="revolute">
    <parent link="child_link"/>
    <child link="child_link2"/>
    <origin xyz="0 0 0.0375" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-3.14159" upper="3.14159" velocity="3"/>
  </joint>
  
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/my_ns</robotNamespace>
    </plugin>
  </gazebo>
  
  <transmission name="trans1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="base_to_child">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
  <transmission name="trans2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="child_to_child2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
</robot>
